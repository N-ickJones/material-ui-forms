lass FormHandler<T> {\r\n  constructor(args: FormArgs<T>) {\r\n    this.controller = args.controller;\r\n    this.key = args.key;\r\n    this.forms = args.state[0];\r\n    this.setForms = args.state[1];\r\n  }\r\n  uuid = Number.MAX_SAFE_INTEGER;\r\n  forms: T[];\r\n  setForms: React.Dispatch<React.SetStateAction<T[]>>;\r\n  key: string;\r\n  controller: IRepoController<T>\r\n\r\n  handleLoad = async (local: boolean, data?: any) => {\r\n    if (local && data) {\r\n      //Test: Do i need to setState here?\r\n      //this.forms = data;\r\n      this.setState(data);\r\n    }\r\n    else {\r\n      const state = await this.controller.getAll();\r\n      if (!state) return false;\r\n      this.setState(state);\r\n    }\r\n    return true;\r\n  }\r\n\r\n  handleAddNewItem = async() => {\r\n    //@ts-ignore\r\n    this.forms.push({ [this.key]: this.uuid });\r\n    this.uuid -= 1;\r\n    this.setForms([...this.forms]);\r\n  }\r\n\r\n  handleDelete = async (index: number): Promise<boolean> => {\r\n    //@ts-ignore\r\n    const residenceId = this.forms[index][this.key];\r\n    if (residenceId !== undefined && isValidId(residenceId)) {\r\n      const response = await this.controller.delete(residenceId);\r\n      if (!response) { return false; }\r\n    }\r\n    setTimeout(() => {\r\n      //@ts-ignore\r\n      this.setState(this.forms.filter((item) => item[this.key]  !== residenceId));\r\n      //this.forms = ;\r\n      //this.setForms(this.forms);\r\n    })\r\n    return true;\r\n  }\r\n\r\n  handleSubmit = async (): Promise<boolean> => {\r\n    let success = true;\r\n    this.forms.forEach(async (object: T, index: number) => {\r\n      //@ts-ignore\r\n      if (object[this.key] !== undefined && isValidId(object[this.key])) {\r\n        //@ts-ignore\r\n        if (!await this.handleUpdate(object[this.key], object)) {\r\n          success = false;\r\n        }\r\n      }\r\n      else {\r\n        if (!await this.handleCreate(index, object)) {\r\n          success = false;\r\n 