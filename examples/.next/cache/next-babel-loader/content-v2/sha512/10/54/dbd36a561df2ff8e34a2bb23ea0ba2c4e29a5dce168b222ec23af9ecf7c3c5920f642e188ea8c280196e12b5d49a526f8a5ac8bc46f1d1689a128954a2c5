sync (local: boolean, data?: any) => {\r\n    if (local && data) {\r\n      this.setState(data);\r\n    }\r\n    else {\r\n      const state = await this.controller.getAll();\r\n      if (!state) return false;\r\n      this.setState(state);\r\n    }\r\n    return true;\r\n  }\r\n\r\n  handleAddNewItem = async () => {\r\n    //@ts-ignore\r\n    this.forms.push({ [this.key]: this.uuid });\r\n    this.uuid -= 1;\r\n    this.args.state[1]([...this.args.state[0]]);\r\n  }\r\n\r\n  handleDelete = async (index: number): Promise<boolean> => {\r\n    //@ts-ignore\r\n    const objectId = this.forms[index][this.key];\r\n    if (objectId !== undefined && isValidId(objectId)) {\r\n      const response = await this.controller.delete(objectId);\r\n      if (!response) { return false; }\r\n    }\r\n    setTimeout(() => {\r\n      //@ts-ignore\r\n      this.setState(this.forms.filter((item) => item[this.key]  !== objectId));\r\n    })\r\n    return true;\r\n  }\r\n\r\n  handleSubmit = async (): Promise<boolean> => {\r\n    let success = true;\r\n    this.args.state[0].forEach(async (object: T, index: number) => {\r\n      //@ts-ignore\r\n      if (object[this.key] !== undefined && isValidId(object[this.key])) {\r\n        //@ts-ignore\r\n        if (!await this.handleUpdate(object[this.key], object)) {\r\n          success = false;\r\n        }\r\n      }\r\n      else {\r\n        if (!await this.handleCreate(index, object)) {\r\n          success = false;\r\n        }\r\n      }\r\n    });\r\n    return success;\r\n  }\r\n\r\n  private handleCreate = async (index: number, object: T): Promise<boolean> => {\r\n    const response = await this.controller.create({ ...object, [this.key]: undefined });\r\n    if (response) {\r\n      //Flush Forms temp key for real key\r\n      const tempForms = this.args.state[0];\r\n      tempForms[index] = response\r\n      this.setState([])\r\n      this.setState(tempForms);\r\n      return true;\r\n    }\r\n    return false;\r\n  }\r\n\r\n  private 